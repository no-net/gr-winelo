<?xml version="1.0"?>
<block>
	<name>mpc_channel_cc</name>
	<key>winelo_mpc_channel_cc</key>
	<category>WiNeLo</category>
	<import>import winelo</import>
	<make>winelo.mpc_channel_cc($taps_delays, $pdp)</make>
<param>
	<name>Num Inputs</name>
	<key>num_inputs</key>
	<value>2</value>
	<type>int</type>
</param>
<param>
	<name>Taps Delays</name>
	<key>taps_delays</key>
	<value>0, 0</value>
	<type>int_vector</type>
</param>
<param>
	<name>Power Delay Profile</name>
	<key>pdp</key>
	<value>1, 0.5</value>
	<type>real_vector</type>
</param>
<sink>
	<name>in</name>
	<type>complex</type>
	<nports>$num_inputs</nports>
</sink>
<source>
	<name>out</name>
	<type>complex</type>
</source>
</block>
