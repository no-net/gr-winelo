<?xml version="1.0"?>
<!--
###################################################
## BPSK Demod, threshold at 0
###################################################
 -->
<block>
	<name>Rayleigh Channel</name>
	<key>winelo_rayleigh_cc</key>
	<category>WiNeLo</category>
	<import>from winelo.channel import rayleigh_cc</import>
	<make>rayleigh_cc($sample_rate, $N, $dopplerspec)</make>
	<param>
		<name>sample rate</name>
		<key>sample_rate</key>
		<value>48000</value>
		<type>int</type>
	</param>
	<param>
		<name>Number of sinusoids</name>
		<key>N</key>
		<value>20</value>
		<type>int</type>
	</param>
	<param>
		<name>Dopplerspectrum</name>
		<key>dopplerspec</key>
		<value>'jakes'</value>
		<type>raw</type>
	</param>
	<sink>
		<name>in</name>
		<type>complex</type>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
	</source>
</block>
