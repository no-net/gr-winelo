<?xml version="1.0"?>
<block>
	<name>evm_cc</name>
	<key>winelo_evm_cc</key>
	<category>WiNeLo</category>
	<import>import winelo</import>
	<make>winelo.evm_cc($vector_length)</make>
	
	<param>
		<name>Vector Length</name>
		<key>vector_length</key>
		<value>100</value>
		<type>int</type>
	</param>

	<sink>
		<name>in0</name>
		<type>complex</type>
	</sink>
	<sink>
		<name>in1</name>
		<type>complex</type>
	</sink>

	<source>
		<name>out</name>
		<type>float</type>
	</source>
<doc>
Computes the error vector magntitude.
</doc>	
</block>
